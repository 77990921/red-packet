{
  "rewrites": [
    {
      "source": "/api/generate",
      "destination": "http://connect.nmb1.seetacloud.com:44854/generate"
    },
    {
      "source": "/api/download",
      "destination": "http://connect.nmb1.seetacloud.com:44854/download"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "POST,GET,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
      ]
    }
  ]
} 