{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://connect.nmb1.seetacloud.com:6006/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,HEAD,PUT,PATCH,POST,DELETE" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Accept, X-Requested-With" },
        { "key": "Access-Control-Max-Age", "value": "86400" }
      ]
    }
  ],
  "regions": ["sfo1"]
} 